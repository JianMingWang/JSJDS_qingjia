@model IEnumerable<qingjia_MVC.Areas.Import.Controllers.ClassModel>
@{
    Layout = null;
    bool flag = false;
    if(Model != null)
    {
        flag = true;   
    }
}
@if (flag)
{
    <table class="table table-condensed">
        <thead>
            <tr>
                <th><input type="checkbox" value=""></th>
                <th>编辑</th>
                <th>账号</th>
                <th>班级</th>
                <th>年级</th>
                <th>负责人</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        <input type="checkbox" value="@item.ID">
                    </td>
                    <td>
                        <input type="image" name="edit" src="~/Content/image/edit.png">
                    </td>
                    <td>@item.ID</td>
                    <td>@item.Name</td>
                    <td>@item.Grade 级 </td>
                    <td>@item.MonitorName</td>
                </tr>
            }
        </tbody>
    </table>
}
else
{
    <div>
        未设定班级！
    </div>
}

